#include <stdio.h>
#include <inttypes.h>

int main() {
    int64_t lnum;
    int64_t fnum;
    int i = 0;
    int temp = 0;
     
    printf("left cost: ");
    scanf("%lld", &lnum);
    
    temp = lnum;
    putchar(temp);
    
    if (temp == EOF){
        printf("\n0");
        return 0;
    }
    

    printf("right cost: ");
    scanf("%lld", &fnum);
    
    if(lnum < -100000000000000000 || lnum > 1000000000000000000|| fnum < -1000000000000000000 || fnum > 1000000000000000000) {
        printf("\n1");
        return 1;
        } 
    
    while(i == 0) {
        
        if(fnum > lnum) {
            printf("Go Left\n");
            printf("You entered %lld and %lld\n", lnum, fnum);
        } else if (lnum > fnum) {
            printf("Go Right\n");
            printf("You entered %lld and %lld\n", lnum, fnum);
        } else {
            printf("Go Left\n");
        }
        printf("left cost: ");
        scanf("%lld", &lnum);
        
        temp = lnum;
        
        if ( temp == EOF) {
        printf("\n0");
        i++;
        i++;
        break;
        }
        
        printf("right cost: ");
        scanf("%lld", &fnum);
        
    
        if(lnum < -100000000000000000 || lnum > 1000000000000000000|| fnum < -1000000000000000000 || fnum > 1000000000000000000) {
        i++;
        break;
        } 
    }
    
    if(i == 1){
        printf("\n1");
        return 1;
    }else if(i == 2){
        printf("\n0");
        return 0;
    }
}
